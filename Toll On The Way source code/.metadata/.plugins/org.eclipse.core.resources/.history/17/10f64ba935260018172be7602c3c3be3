from django.http import HttpResponse

def index(request):
    empss=employdetails.objects.all()
    html=''
    for em in empss:
        url='/emp/' + str(em.id) + '/'
        html+='<a href="'+url+'">'+ em.employname+'<a/><br>'
    return HttpResponse(html)
   

