from django.db import models
from django.contrib.auth.models import User
class profile(models.Model):
    name=models.CharField(max_length=120)
    
class Post(models.Model):
    first=models.CharField(error_messages={'required':'please enter'},max_length=500)
    last=models.CharField(error_messages={'required':'please enter'},max_length=500,null=True)
    vehicleno=models.CharField(error_messages={'required':'please enter'},max_length=500,null=True)
    vehicletype=models.CharField(error_messages={'required':'please enter'},max_length=500,null=True)
    journey_type=models.CharField(max_length=500,null=True)
    user=models.ForeignKey(User)    

    

